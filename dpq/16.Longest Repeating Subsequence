q: 
soln:
class Solution {
	public:
		int LongestRepeatingSubsequence(string s1){
		    int x=s1.length();
		    int y=x;
		    string s2=s1;
		    int dp[x+1][y+1];
    
    int ans=0;
int i,j;
for(i=0;i<=x;i++)
{
    for(j=0;j<=y;j++)
    {
        if(i==0||j==0)dp[i][j]=0;
    }
}
for(i=1;i<=x;i++)
{
    for(j=1;j<=y;j++)
    {
    {
        if(s1[i-1]==s2[j-1] && i!=j )
        {
            dp[i][j]=1+dp[i-1][j-1];
           // ans=max(ans,dp[i][j]);
            
        }
        else
        {
            dp[i][j]=max( dp[i-1][j],dp[i][j-1]);
            
        }
        
        
        
    }
    }
}

	return dp[x][y];	    
		    
		}

};
