
#include <bits/stdc++.h>
using namespace std;



 int solve(int a[],int sum,int n)
{
    int t[n+1][sum+1];
    int i,j;
    for(i=0;i<n+1;i++)
    {
        for(j=0;j<=sum;j++)
        {
            if(j==0)t[i][j]=1;
            else if(i==0)t[i][j]=0;
            else 
            {
                if(a[i-1]>j)
                t[i][j]=t[i-1][j];
                else
                {
                    t[i][j]=t[i-1][j]+t[i-1][j-a[i-1]];
                }
            }
        }
    }
    return t[n][sum];
}

int knapSack(int a[],int n,int d)
{ int sum=0;
    int i;
    for(i=0;i<n;i++)sum+=a[i];
    sum=(sum+d)/2;
    if(sum%2!=0)return 0;
    int counter=0;
    counter=solve(a,n,sum);
    return counter;
}

int main()
{
    int n;
    cin>>n;
    int i;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int d;cin>>d;
    int ans=knapSack(a,n,d);
    cout<<ans;
    return 0;
    
}

