	int rowWithMax1s(vector<vector<int> > arr, int n, int m) {
	    int ans=-1;
	    int row,col;
	    row=0;
	    col=m-1;
	    while(row<n && col>=0)
	    {
	        if(arr[row][col]==1)
	        {
	            ans=row;
	            col--;
	        }
	        else
	        row++;
	        
	    }
	    return ans;
	    
	}
