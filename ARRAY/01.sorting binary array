
void binSort(int A[], int N)
    {
       int i=0,j=N-1;
       while(i<j)
       {
           while( i<=j && A[i]!=1)
           i++;
           
           while(j>=i && A[j]!=0)
           j--;
           if(i<j)
       {
           int temp=A[j];
           A[j]=A[i];
           A[i]=temp;
       }
           
           
       }
       
    }
};
