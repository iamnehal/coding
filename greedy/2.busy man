https://www.spoj.com/problems/BUSYMAN/



#include<bits/stdc++.h>
using namespace std;
int main()
{
int t;
cin>>t;
while(t--)
{
	

vector<pair<int,int> >v;
int n;
cin>>n;
int i=0;

for(i=0;i<n;i++)
{int temp;
cin>>temp;
int temp1;
cin>>temp1;
	v.push_back(make_pair(temp1,temp));
}
sort(v.begin(),v.end());
int endtime=0;
int count=0;
for(i=0;i<n;i++)
{
	if(v[i].second>=endtime)
	{
	endtime=v[i].first;
	count++;
	
	}


}

cout<<count<<endl;


} return 0;
}
