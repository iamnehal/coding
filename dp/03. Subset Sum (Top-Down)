https://www.interviewbit.com/problems/subset-sum-problem/

int Solution::solve(vector<int> &A, int B) {
    int dp[A.size()+1][B+1],N=A.size();
    
    for(int i=0;i<=N;i++){
        for(int j=0;j<=B;j++){
            if(i==0)
                dp[i][j]=0;
            if(j==0)
                dp[j][i]=1;
        }
    }
     for(int i=1;i<=N;i++){
        for(int j=1;j<=B;j++){
           if(A[i-1]<=j){
                dp[i][j]=(dp[i-1][j-A[i-1]] || dp[i-1][j]);
                }
                else if(A[i-1]>j){
                    dp[i][j]=dp[i-1][j];
                } 
        }
    }
    
    return dp[N][B];
}
