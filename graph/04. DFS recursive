void dfs(vector<int> a[],int s,bool *v,vector<int> &ans)
{
    
    v[s]=true;//marking visited node as true 
    ans.push_back(s);//storing value in vector 
    for(int i=0;i<a[s].size();i++)
    {
        if(v[a[s][i]]==false)
        {
            dfs(a,a[s][i],v,ans);
            
        }
    }
    
    
    
    
}

	vector<int>dfsOfGraph(int n, vector<int> adj[]){
	    
	    bool vis[n]={false};
	    vector<int> ans;
	    dfs(adj,0,vis,ans);
	    return ans;
	    
	    
	}
