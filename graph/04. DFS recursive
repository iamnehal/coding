void dfs(vector<int>adj[],int s,vector<int> &vis,vector<int> &ans)
	{
	    
	    vis[s]=1;//marking visited nodes as 1
	    ans.push_back(s);
	    for(auto x:adj[s])
	    { if(vis[x]==0)
	    {
	        dfs(adj,x,vis,ans);
	    }
	        
	    }
	    return ;
	}
	
	
	vector<int>dfsOfGraph(int V, vector<int> adj[])
	{
	  
	  vector<int>vis(V,0);
	  vector<int>ans;
	  dfs(adj,0,vis,ans);
	  return ans;
	}
};
