https://www.codechef.com/CCRC21C/problems/CHEFPAT

#include <iostream>
#include<math.h> 
#include <algorithm>
#include<vector> 

using namespace std;
int main()
{
  int t;
  cin>>t;

while(t--)
{ int i=0;
  int n; int temp;
  cin>>n;
  vector<int>a[1001];
  for(int i=0;i<n;i++)
  {
      cin>>temp;
      a[temp].push_back(i);
  } int time=0; int j=0; int s=0; int ans[n];
  for(i=1000;i>=0;i--)
  {s=a[i].size();
  for(j=0;j<s;j++)
  { time++;
      ans[a[i][j]]=time;
  }
  
      
  }
  for(i=0;i<n;i++)
  {
      cout<<ans[i]<<endl;
  }
  



}
return 0;
}
