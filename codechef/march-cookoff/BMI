https://www.codechef.com/CCRC21C/problems/BMI
#include <iostream>
#include<math.h> 
using namespace std;
int main()
{
  int t;
cin>>t;
while(t--)
{
double m,h;
  cin>>m>>h;

int digit=0;
  int ans=0;
   ans=(m)/(pow(h,2));
   if(ans<=18)digit=1;
   else
   {
    if(ans<=24)digit=2;
   
   else if(ans<=29)digit=3;
   else digit=4;
}

cout<<digit<<endl;
}
return 0;
}
